  <variable name="FanartToggleVar">
    <value condition="Container.Content(movies) + String.IsEqual(Skin.String(ArtworkChoice.movies),0)">Skin.SetString(ArtworkChoice.movies,1)</value>
    <value condition="Container.Content(movies) + String.IsEqual(Skin.String(ArtworkChoice.movies),1)">Skin.SetString(ArtworkChoice.movies,0)</value>
    <value condition="Container.Content(tvshows) + String.IsEqual(Skin.String(ArtworkChoice.tvshows),0)">Skin.SetString(ArtworkChoice.tvshows,1)</value>
    <value condition="Container.Content(tvshows) + String.IsEqual(Skin.String(ArtworkChoice.tvshows),1)">Skin.SetString(ArtworkChoice.tvshows,0)</value>
    <value condition="Container.Content(seasons) + String.IsEqual(Skin.String(ArtworkChoice.seasons),0)">Skin.SetString(ArtworkChoice.seasons,1)</value>
    <value condition="Container.Content(seasons) + String.IsEqual(Skin.String(ArtworkChoice.seasons),1)">Skin.SetString(ArtworkChoice.seasons,0)</value>
    <value condition="Container.Content(episodes) + String.IsEqual(Skin.String(ArtworkChoice.episodes),0)">Skin.SetString(ArtworkChoice.episodes,1)</value>
    <value condition="Container.Content(episodes) + String.IsEqual(Skin.String(ArtworkChoice.episodes),1)">Skin.SetString(ArtworkChoice.episodes,0)</value>
  </variable>
  
  <variable name="FanartSettingVar">
    <value condition="[[Container.Content(movies) + String.IsEqual(Skin.String(ArtworkChoice.movies),0)] | [Container.Content(tvshows) + String.IsEqual(Skin.String(ArtworkChoice.tvshows),0)] | [Container.Content(seasons) + String.IsEqual(Skin.String(ArtworkChoice.seasons),0)] | [Container.Content(episodes) + String.IsEqual(Skin.String(ArtworkChoice.episodes),0)]]">Fanart</value>
    <value condition="[[Container.Content(movies) + String.IsEqual(Skin.String(ArtworkChoice.movies),1)] | [Container.Content(tvshows) + String.IsEqual(Skin.String(ArtworkChoice.tvshows),1)] | [Container.Content(seasons) + String.IsEqual(Skin.String(ArtworkChoice.seasons),1)] | [Container.Content(episodes) + String.IsEqual(Skin.String(ArtworkChoice.episodes),1)]]">Landscape</value>
  </variable>	
  
  <variable name="LandscapeFanartToggleVar">
    <value condition="Container.Content(movies) + String.IsEqual(Skin.String(LandscapeFanart.movies),0)">Skin.SetString(LandscapeFanart.movies,1)</value>
    <value condition="Container.Content(movies) + String.IsEqual(Skin.String(LandscapeFanart.movies),1)">Skin.SetString(LandscapeFanart.movies,0)</value>
    <value condition="Container.Content(tvshows) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),0)">Skin.SetString(LandscapeFanart.tvshows,1)</value>
    <value condition="Container.Content(tvshows) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),1)">Skin.SetString(LandscapeFanart.tvshows,0)</value>
    <value condition="Container.Content(seasons) + String.IsEqual(Skin.String(LandscapeFanart.seasons),0)">Skin.SetString(LandscapeFanart.seasons,1)</value>
    <value condition="Container.Content(seasons) + String.IsEqual(Skin.String(LandscapeFanart.seasons),1)">Skin.SetString(LandscapeFanart.seasons,0)</value>
    <value condition="Container.Content(episodes) + String.IsEqual(Skin.String(LandscapeFanart.episodes),0)">Skin.SetString(LandscapeFanart.episodes,1)</value>
    <value condition="Container.Content(episodes) + String.IsEqual(Skin.String(LandscapeFanart.episodes),1)">Skin.SetString(LandscapeFanart.episodes,0)</value>
  </variable>
  
  <variable name="LandscapeFanartSettingVar">
    <value condition="[[Container.Content(movies) + String.IsEqual(Skin.String(LandscapeFanart.movies),0)] | [Container.Content(tvshows) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),0)] | [Container.Content(seasons) + String.IsEqual(Skin.String(LandscapeFanart.seasons),0)] | [Container.Content(episodes) + String.IsEqual(Skin.String(LandscapeFanart.episodes),0)]]">Default</value>
    <value condition="[[Container.Content(movies) + String.IsEqual(Skin.String(LandscapeFanart.movies),1)] | [Container.Content(tvshows) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),1)] | [Container.Content(seasons) + String.IsEqual(Skin.String(LandscapeFanart.seasons),1)] | [Container.Content(episodes) + String.IsEqual(Skin.String(LandscapeFanart.episodes),1)]]">Fanart</value>
  </variable>
  
  <variable name="HomeFlixLandscapeVar">
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),1) + String.IsEqual(Skin.String(HomeLandscapeFanart),0)">$INFO[ListItem.Art(landscape)]</value>
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),1) + String.IsEqual(Skin.String(HomeLandscapeFanart),1)">$INFO[ListItem.Art(fanart)]</value>
    <value condition="!String.isempty(ListItem.Thumb) + $EXP[isEpisode]">$INFO[ListItem.Thumb]</value>
    <value condition="Window.IsActive(musicinformation) | Window.IsActive(movieinformation) | Window.IsActive(addoninformation) | Window.IsActive(pvrguideinfo) | Window.IsActive(pvrrecordinginfo) | Window.IsActive(pictureinfo)">$INFO[Window(home).Property(infobackground)]</value>
    <value condition="Container.Content(genres) + Window.IsActive(videos)">$INFO[Skin.String(MovieGenreFanart.path)]$INFO[ListItem.Label]$INFO[Skin.String(MovieGenreFanart.ext)]</value>
    <value condition="Window.IsActive(pictures) + String.IsEmpty(ListItem.Art(fanart))">$INFO[ListItem.FolderPath]</value>
    <value condition="!String.IsEmpty(ListItem.Art(fanart))">$INFO[ListItem.Art(fanart)]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_fanart_art))">$INFO[Window(home).Property(nimbus_fanart_art)]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_fanart_prop))">$INFO[Window(home).Property(nimbus_fanart_prop)]</value>
    <value condition="!String.IsEmpty(Container.ListItem.Property(FanartCode))">$INFO[Skin.String(WeatherFanart.path)]$INFO[Container.ListItem.Property(FanartCode)]$INFO[Skin.String(WeatherFanart.ext)]</value>
    <value condition="!String.IsEmpty(Container.Art(tvshow.fanart))">$INFO[Container.Art(tvshow.fanart)]</value>
    <value condition="!String.IsEmpty(Container.Art(artist.fanart))">$INFO[Container.Art(artist.fanart)]</value>
    <value>$INFO[Container.Art(fanart)]</value>
  </variable>
  
  <variable name="HomeFanartToggleVar">
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),0)">Skin.SetString(HomeArtworkChoice,1)</value>
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),1)">Skin.SetString(HomeArtworkChoice,0)</value>
  </variable>
  
  <variable name="HomeFanartSettingVar">
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),0)">Fanart</value>
    <value condition="String.IsEqual(Skin.String(HomeArtworkChoice),1)">Landscape</value>
  </variable>
  
  <variable name="HomeLandscapeFanartToggleVar">
    <value condition="String.IsEqual(Skin.String(HomeLandscapeFanart),0)">Skin.SetString(HomeLandscapeFanart,1)</value>
    <value condition="String.IsEqual(Skin.String(HomeLandscapeFanart),1)">Skin.SetString(HomeLandscapeFanart,0)</value>
  </variable>
  
  <variable name="HomeLandscapeFanartSettingVar">
    <value condition="String.IsEqual(Skin.String(HomeLandscapeFanart),0)">Default</value>
    <value condition="String.IsEqual(Skin.String(HomeLandscapeFanart),1)">Fanart</value>
  </variable>
  
  <variable name="BlurStateVar">
    <value condition="!Skin.HasSetting(Enable.BackgroundBlue)">Disabled</value>
    <value condition="Skin.HasSetting(Enable.BackgroundBlue)">Enabled</value>
  </variable>
</includes>