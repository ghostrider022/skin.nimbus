  <variable name="HomeFanartVar">
    <value condition="!String.IsEmpty(ListItem.Thumb) + $EXP[isEpisode]">$INFO[ListItem.Thumb]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_episode_thumb))">$INFO[Window(home).Property(nimbus_episode_thumb)]</value>
    <value condition="[!String.IsEmpty(ListItem.Art(fanart)) + [!String.IsEqual(Skin.String(HomeArtworkChoice),1) | String.IsEqual(Skin.String(HomeLandscapeFanart),1)]] | [String.IsEqual(Skin.String(HomeArtworkChoice),1) + String.IsEqual(Skin.String(HomeLandscapeFanart),0) + String.IsEmpty(ListItem.Art(landscape)) + !String.IsEmpty(ListItem.Art(fanart))] | [!$EXP[isContent] + !String.IsEmpty(ListItem.Art(fanart))]">$INFO[ListItem.Art(fanart)]</value>
    <value condition="!String.IsEmpty(ListItem.Art(landscape)) + String.IsEqual(Skin.String(HomeArtworkChoice),1) + String.IsEqual(Skin.String(HomeLandscapeFanart),0)">$INFO[ListItem.Art(landscape)]</value>
    <value condition="Window.IsActive(musicinformation) | Window.IsActive(movieinformation) | Window.IsActive(addoninformation) | Window.IsActive(pvrguideinfo) | Window.IsActive(pvrrecordinginfo) | Window.IsActive(pictureinfo)">$INFO[Window(home).Property(infobackground)]</value>
    <value condition="Container.Content(genres) + Window.IsActive(videos)">$INFO[Skin.String(MovieGenreFanart.path)]$INFO[ListItem.Label]$INFO[Skin.String(MovieGenreFanart.ext)]</value>
    <value condition="Window.IsActive(pictures) + String.IsEmpty(ListItem.Art(fanart))">$INFO[ListItem.FolderPath]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_fanart_art)) + [!String.IsEqual(Skin.String(HomeArtworkChoice),1) | String.IsEqual(Skin.String(HomeLandscapeFanart),1)]">$INFO[Window(home).Property(nimbus_fanart_art)]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_fanart_prop)) + [!String.IsEqual(Skin.String(HomeArtworkChoice),1) | String.IsEqual(Skin.String(HomeLandscapeFanart),1)]">$INFO[Window(home).Property(nimbus_fanart_prop)]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_landscape_art)) + String.IsEqual(Skin.String(HomeLandscapeFanart),0)">$INFO[Window(home).Property(nimbus_landscape_art)]</value>
    <value condition="!String.IsEmpty(Window(home).Property(nimbus_landscape_prop)) + String.IsEqual(Skin.String(HomeLandscapeFanart),0)">$INFO[Window(home).Property(nimbus_landscape_prop)]</value>
    <value condition="!String.IsEmpty(Container.ListItem.Property(FanartCode)) + String.IsEqual(Skin.String(HomeArtworkChoice),0)">$INFO[Skin.String(WeatherFanart.path)]$INFO[Container.ListItem.Property(FanartCode)]$INFO[Skin.String(WeatherFanart.ext)]</value>
    <value condition="!String.IsEmpty(Container.Art(tvshow.fanart))">$INFO[Container.Art(tvshow.fanart)]</value>
    <value condition="!String.IsEmpty(Container.Art(artist.fanart))">$INFO[Container.Art(artist.fanart)]</value>
    <value>$INFO[Container.Art(fanart)]</value>
</variable>

	<variable name="MediaFanartVar">
		<value condition="!String.IsEmpty(ListItem.Thumb) + $EXP[isEpisode]">$INFO[ListItem.Thumb]</value>
		<value condition="!String.IsEmpty(ListItem.Art(fanart))">$INFO[ListItem.Art(fanart)]</value>
		<value condition="!String.IsEmpty(Container.Art(tvshow.fanart))">$INFO[Container.Art(tvshow.fanart)]</value>
		<value condition="!String.IsEmpty(Container.Art(artist.fanart))">$INFO[Container.Art(artist.fanart)]</value>
		<value>$INFO[Container.Art(fanart)]</value>
	</variable>

  <variable name="AddonsFanartVar">
		<value condition="Window.IsActive(musicinformation) | Window.IsActive(movieinformation) | Window.IsActive(addoninformation) | Window.IsActive(pvrguideinfo) | Window.IsActive(pvrrecordinginfo) | Window.IsActive(pictureinfo)">$INFO[Window(home).Property(infobackground)]</value>
		<value condition="!String.IsEmpty(Container.ListItem.Art(fanart))">$INFO[Container.ListItem.Art(fanart)]</value>
		<value condition="!String.IsEmpty(Skin.String(HomeFanart.path))">$INFO[Skin.String(HomeFanart.path)]$INFO[Container(9000).ListItem.Property(id)]$INFO[Skin.String(HomeFanart.ext)]</value>
	</variable>
  
	<variable name="WeatherFanartVar">
		<value condition="!String.IsEmpty(Container.ListItem.Property(FanartCode))">$INFO[Skin.String(WeatherFanart.path)]$INFO[Container.ListItem.Property(FanartCode)]$INFO[Skin.String(WeatherFanart.ext)]</value>
		<value condition="!String.IsEmpty(Window(weather).Property(current.fanartcode)) + !String.IsEmpty(Skin.String(weatherfanart.path))">$INFO[Skin.String(weatherfanart.path)]$INFO[Window(Weather).Property(Current.FanartCode)]$INFO[Skin.String(weatherfanart.ext)]</value>
		<value condition="!String.IsEmpty(Skin.String(HomeFanart.path))">$INFO[Skin.String(HomeFanart.path)]weather$INFO[Skin.String(HomeFanart.ext)]</value>
	</variable>

  <variable name="FlixLandscapeVar">
    <value condition="$EXP[isMovie] + String.IsEqual(Skin.String(ArtworkChoice.movies),1) + String.IsEqual(Skin.String(LandscapeFanart.movies),0)">$INFO[Listitem.Art(landscape)]</value>
    <value condition="$EXP[isMovie] + String.IsEqual(Skin.String(ArtworkChoice.movies),1) + String.IsEqual(Skin.String(LandscapeFanart.movies),1)">$INFO[ListItem.Art(fanart)]</value>
    <value condition="$EXP[isTVShow] + String.IsEqual(Skin.String(ArtworkChoice.tvshows),1) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),0)">$INFO[Listitem.Art(landscape)]</value>
    <value condition="$EXP[isTVShow] + String.IsEqual(Skin.String(ArtworkChoice.tvshows),1) + String.IsEqual(Skin.String(LandscapeFanart.tvshows),1)">$INFO[ListItem.Art(fanart)]</value>
    <value condition="$EXP[isSeason] + String.IsEqual(Skin.String(ArtworkChoice.seasons),1) + String.IsEqual(Skin.String(LandscapeFanart.seasons),0)">$INFO[Listitem.Art(landscape)]</value>
    <value condition="$EXP[isSeason] + String.IsEqual(Skin.String(ArtworkChoice.seasons),1) + String.IsEqual(Skin.String(LandscapeFanart.seasons),1)">$INFO[ListItem.Art(fanart)]</value>
    <value condition="$EXP[isEpisode] + String.IsEqual(Skin.String(ArtworkChoice.episodes),1) + String.IsEqual(Skin.String(LandscapeFanart.episodes),0) + !String.IsEmpty(ListItem.Thumb)">$INFO[ListItem.Thumb]</value>
    <value condition="$EXP[isEpisode] + String.IsEqual(Skin.String(ArtworkChoice.episodes),1) + String.IsEqual(Skin.String(LandscapeFanart.episodes),1) + !String.IsEmpty(ListItem.Art(fanart))">$INFO[ListItem.Art(fanart)]</value>
    <value condition="$EXP[isEpisode] + String.IsEqual(Skin.String(ArtworkChoice.episodes),0)">$INFO[ListItem.Thumb]</value>
    <value>$INFO[ListItem.Art(fanart)]</value>
  </variable>

